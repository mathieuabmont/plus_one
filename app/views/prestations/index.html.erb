<div class="container">

  <div class="content-map">
    <div class="row">
      <div class="card">
        <% @prest.each do |prestation| %>
          <div class="col-xs-12 col-sm-4">
            <%= render 'shared/card', prestation: prestation %>
          </div>
        <% end %>
      </div>
    </div>

    <div id="map" style="width: 100%;height: 600px;" data-markers="<%= @markers.to_json %>">
    </div>
  </div>

</div>

